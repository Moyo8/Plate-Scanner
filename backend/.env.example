# Copy this file to .env and fill values for Mailtrap and other envs
# MAILTRAP_TOKEN is required for Mailtrap API calls
MAILTRAP_TOKEN=
MAILTRAP_SENDER_EMAIL=hello@yourdomain.com
MAILTRAP_SENDER_NAME=PlateScanner
# The client frontend URL used in verification/reset links (frontend origin)
CLIENT_URL=http://localhost:3000
# JWT secret for signing tokens
JWT_SECRET=replace_with_secure_secret
NODE_ENV=development
